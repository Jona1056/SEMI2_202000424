        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
          
            try
            {
                string delimitador = Dts.Variables["User::Delimitador"].Value.ToString();
                string Extension_compra = Dts.Variables["User::Extension_compra"].Value.ToString();
                string Extension_venta = Dts.Variables["User::Extension_venta"].Value.ToString();
                string FolderOrigen = Dts.Variables["User::FolderOrigen"].Value.ToString();
                string FolderError = Dts.Variables["User::FolderError"].Value.ToString();
                string pivote_compra = Dts.Variables["User::TablaDestinoCompra"].Value.ToString();
                string pivote_venta = Dts.Variables["User::TablaDestinoVenta"].Value.ToString();

                string[] fileentries_compra = Directory.GetFiles(FolderOrigen, "*" + Extension_compra);

                string filename_compra = fileentries_compra[1];
                string[] fileentries_venta = Directory.GetFiles(FolderOrigen, "*" + Extension_venta);
                string filename_venta = fileentries_venta[1];
                int contador = 0;
                string linea;

                using (StreamReader SourceFile = new StreamReader(filename_compra, Encoding.UTF8))
                {
                    while ((linea = SourceFile.ReadLine()) != null)
                    {
                        if (contador > 0)
                        {
                            string[] campos = linea.Split(delimitador.ToCharArray()[0]);
                            string ruta = "Data Source=.\\SQLEXPRESS;Initial Catalog=BASE2_SEMI2;Integrated Security=True";

                            string query = "INSERT INTO " + pivote_compra + "(Fecha, CodProveedor, NombreProveedor, DireccionProveedor, NumeroProveedor,WebProveedor, CodProducto, NombreProducto, MarcaProducto, Categoria,CodSucursal, NombreSucursal, DireccionSucursal, Region, Departamento, Unidades, CostoU ) VALUES (   @Fecha, @CodProveedor, @NombreProveedor, @DireccionProveedor, @NumeroProveedor,@WebProveedor, @CodProducto, @NombreProducto, @MarcaProducto, @Categoria, @CodSucursal, @NombreSucursal, @DireccionSucursal, @Region, @Departamento,@Unidades, @CostoU)";
                          
                            using (SqlConnection connection = new SqlConnection(ruta))
                            {
                                using (SqlCommand myCommand = new SqlCommand(query, connection))
                                {
                                    string fechaOriginal = campos[0];
                                    DateTime fechaConvertida = DateTime.ParseExact(fechaOriginal, "dd/MM/yyyy", null);
                                    string fechaFormateada = fechaConvertida.ToString("yyyy-MM-dd");
                                    myCommand.Parameters.AddWithValue("@Fecha", fechaFormateada);
                                    myCommand.Parameters.AddWithValue("@CodProveedor", campos[1]);
                                    myCommand.Parameters.AddWithValue("@NombreProveedor", campos[2]);
                                    string direccionProveedor = campos[3].Replace("'", "''");
                                    myCommand.Parameters.AddWithValue("@DireccionProveedor", campos[3]);
                                    myCommand.Parameters.AddWithValue("@NumeroProveedor", campos[4]);
                                    myCommand.Parameters.AddWithValue("@WebProveedor", campos[5]);
                                    myCommand.Parameters.AddWithValue("@CodProducto", campos[6]);
                                    myCommand.Parameters.AddWithValue("@NombreProducto", campos[7]);
                                    myCommand.Parameters.AddWithValue("@MarcaProducto", campos[8]);
                                    myCommand.Parameters.AddWithValue("@Categoria", campos[9]);
                                    myCommand.Parameters.AddWithValue("@CodSucursal", campos[10]);
                                    myCommand.Parameters.AddWithValue("@NombreSucursal", campos[11]);
                                    myCommand.Parameters.AddWithValue("@DireccionSucursal", campos[12]);
                                    myCommand.Parameters.AddWithValue("@Region", campos[13]);
                                    myCommand.Parameters.AddWithValue("@Departamento", campos[14]);
                                    myCommand.Parameters.AddWithValue("@Unidades", campos[15]); 
                                    myCommand.Parameters.AddWithValue("@CostoU",campos[16]);

                                    myCommand.CommandType = CommandType.Text;
                                    connection.Open();
                                    myCommand.ExecuteNonQuery();
                                    connection.Close();
                                }
                            }

                        }
                        contador++;
                    }
                    SourceFile.Close();
                  
                }
                contador = 0;
                using (StreamReader SourceFile = new StreamReader(filename_venta, Encoding.UTF8))
                {
                    while ((linea = SourceFile.ReadLine()) != null)
                    {
                        if (contador > 0)
                        {
                            string[] campos = linea.Split(delimitador.ToCharArray()[0]);
                            string ruta = "Data Source=.\\SQLEXPRESS;Initial Catalog=BASE2_SEMI2;Integrated Security=True";

                            string query = "INSERT INTO " + pivote_venta + "(Fecha, CodigoCliente, NombreCliente, TipoCliente, DireccionCliente, NumeroCliente, CodVendedor, NombreVendedor, Vacacionista, CodProducto, NombreProducto, MarcaProducto, Categoria, CodSucursal, NombreSucursal, DireccionSucursal, Region, Departamento, Unidades, PrecioUnitario) " +
               "VALUES (@Fecha, @CodigoCliente, @NombreCliente, @TipoCliente, @DireccionCliente, @NumeroCliente, @CodVendedor, @NombreVendedor, @Vacacionista, @CodProducto, @NombreProducto, @MarcaProducto, @Categoria, @CodSucursal, @NombreSucursal, @DireccionSucursal, @Region, @Departamento, @Unidades, @PrecioUnitario)";


                            using (SqlConnection connection = new SqlConnection(ruta))
                            {
                                using (SqlCommand myCommand = new SqlCommand(query, connection))
                                {
                                  
                                    string fechaOriginal = campos[0];
                                    DateTime fechaConvertida = DateTime.ParseExact(fechaOriginal, "dd/MM/yyyy", null);
                                    myCommand.Parameters.AddWithValue("@Fecha", fechaConvertida); 
                                    myCommand.Parameters.AddWithValue("@CodigoCliente", campos[1]);
                                    myCommand.Parameters.AddWithValue("@NombreCliente", campos[2]);
                                    myCommand.Parameters.AddWithValue("@TipoCliente", campos[3]);
                                    string direccionCliente = campos[4];
                                    myCommand.Parameters.AddWithValue("@DireccionCliente", direccionCliente);

                                    myCommand.Parameters.AddWithValue("@NumeroCliente", campos[5]);
                                    myCommand.Parameters.AddWithValue("@CodVendedor", campos[6]);
                                    myCommand.Parameters.AddWithValue("@NombreVendedor", campos[7]);
                                    if (campos[8] == "NAC"){
                                        campos[8] = "0";
                                    }
                                    myCommand.Parameters.AddWithValue("@Vacacionista", campos[8]);
                                    myCommand.Parameters.AddWithValue("@CodProducto", campos[9]);
                                    myCommand.Parameters.AddWithValue("@NombreProducto", campos[10]);
                                    myCommand.Parameters.AddWithValue("@MarcaProducto", campos[11]);
                                    myCommand.Parameters.AddWithValue("@Categoria", campos[12]);
                                    myCommand.Parameters.AddWithValue("@CodSucursal", campos[13]);
                                    myCommand.Parameters.AddWithValue("@NombreSucursal", campos[14]);
                                    myCommand.Parameters.AddWithValue("@DireccionSucursal", campos[15]);
                                    myCommand.Parameters.AddWithValue("@Region", campos[16]);
                                    myCommand.Parameters.AddWithValue("@Departamento", campos[17]);
                                    myCommand.Parameters.AddWithValue("@Unidades", campos[18]);
                                    myCommand.Parameters.AddWithValue("@PrecioUnitario", campos[19]);

                                    myCommand.CommandType = CommandType.Text;
                                    connection.Open();
                                    myCommand.ExecuteNonQuery();
                                    connection.Close();
                                }
                            }

                        }
                        contador++;
                    }
                    SourceFile.Close();
                    Dts.TaskResult = (int)ScriptResults.Success;
                }
            }
            catch (Exception ex)
            {
                using (StreamWriter sw = File.CreateText(Dts.Variables["User::FolderError"].Value.ToString() + "\\" + "ErrorLog01.log"))
                {


                    sw.WriteLine(ex.ToString());
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }

            }

                
			Dts.TaskResult = (int)ScriptResults.Success;
		}

